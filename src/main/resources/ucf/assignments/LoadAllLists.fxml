<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ucf.assignments.LoadAllListsController">
   <Text layoutX="200.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Location">
      <font>
         <Font size="36.0"/>
      </font>
   </Text>
   <TextField fx:id="FileLocationTextField" layoutX="225.0" layoutY="175.0"/>
   <Button fx:id="BrowseButton" layoutX="255.0" layoutY="225.0" mnemonicParsing="false" onAction="#BrowseForLoadFile"
           text="Browse">
      <font>
         <Font size="20.0"/>
      </font>
   </Button>
   <Button fx:id="LoadButton" layoutX="267.0" layoutY="305.0" mnemonicParsing="false"
           onAction="#LoadAllListButtonClicked" prefHeight="25.0" prefWidth="67.0" text="Load">
      <font>
         <Font size="16.0"/>
      </font>
   </Button>
</AnchorPane>
